{
  "name": "@commercetools-us-ps/mc-app-core",
  "version": "1.1.24",
  "description": "React components and utilities for building MC custom applications",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/commercetools/mc-custom-applications-core"
  },
  "main": "index.js",
  "module": "index.es.js",
  "jsnext:main": "index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "prebuild": "rimraf dist/**",
    "precommit": "lint-staged",
    "lint": "jest --projects jest.eslint.config.js jest.eslint-graphql.config.js jest.stylelint.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:graphql": "jest --config jest.eslint-graphql.config.js --passWithNoTests",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format": "prettier --write '**/*.{js,css,json}'",
    "format:js": "prettier --write '**/*.js'",
    "format:css": "prettier --write '**/*.css'",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "test:coverage": "jest --config jest.test.config.js --coverage --collectCoverageFrom=components/**/*.js --collectCoverageFrom=!'**/(index|messages).js'",
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:prod": "yarn run build",
    "publish-release": "cd dist && yarn publish --registry=https://registry.npmjs.org/ && cd ..",
    "create-release": "yarn version",
    "preversion": "git stash && git clean -fd . && git checkout master && git pull",
    "postversion": "git commit -am \"chore(release): version $npm_package_version\" && git push && git push --tags && echo \"Successfully released version $npm_package_version!\" && yarn run open-releases",
    "open-releases": "open \"$(node -e 'console.log(`${require(\"./package.json\").repository.url}/releases`)')\""
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@babel/runtime-corejs3": "7.8.4",
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "apollo-cache": "1.3.4",
    "apollo-client": "2.6.8",
    "apollo-link": "1.2.13",
    "apollo-utilities": "1.3.3",
    "aws-sdk": "2.610.0",
    "classnames": "2.2.6",
    "lodash": "4.17.15",
    "lodash-es": "4.17.15",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "7.8.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commercetools-frontend/actions-global": "16.2.1",
    "@commercetools-frontend/application-shell-connectors": "16.2.1",
    "@commercetools-frontend/babel-preset-mc-app": "16.2.1",
    "@commercetools-frontend/constants": "16.0.0",
    "@commercetools-frontend/eslint-config-mc-app": "16.0.0",
    "@commercetools-frontend/jest-preset-mc-app": "16.2.1",
    "@commercetools-frontend/ui-kit": "10.16.0",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@rollup/plugin-multi-entry": "^3.0.1",
    "@testing-library/react": "9.4.0",
    "@types/react": "16.9.19",
    "@types/react-redux": "7.1.7",
    "babel-eslint": "10.0.3",
    "babel-plugin-import-graphql": "2.7.0",
    "babel-plugin-react-intl": "5.1.18",
    "babel-plugin-transform-rename-import": "2.3.0",
    "cross-env": "7.0.0",
    "enzyme": "3.11.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-graphql": "3.1.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "faker": "4.1.0",
    "graphql": "14.6.0",
