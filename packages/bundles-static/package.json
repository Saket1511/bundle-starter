    "@commercetools-frontend/eslint-config-mc-app": "16.15.3",
    "@commercetools-frontend/jest-preset-mc-app": "20.4.0",
    "@commercetools-frontend/mc-scripts": "20.5.0",
    "@testing-library/react": "11.2.7",
    "apollo-cache-inmemory": "1.6.6",
