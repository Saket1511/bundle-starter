{
  "name": "@commercetools-us-ps/bundles-static",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "compile-html:local": "NODE_ENV=production MC_APP_ENV=development dotenv -- mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "yarn compile-html:local && mc-scripts serve",
    "compile-html:firebase": "cross-env FIREBASE_TARGET=bundles-manager mc-scripts compile-html --headers=$(pwd)/production-firebase.headers.json --config=$(pwd)/production-firebase.env.json --use-local-assets --transformer $(pwd)/config/transformer-firebase.js",
    "precommit": "lint-staged",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data '{src,../bundles-core,../core}/**/!(*.spec).js' --build-translations",
    "lint": "jest --projects jest.eslint.config.js jest.eslint-graphql.config.js jest.stylelint.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:graphql": "jest --config jest.eslint-graphql.config.js --passWithNoTests",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format": "prettier --write '**/*.{js,css,gql,graphql,json}'",
    "format:js": "prettier --write '**/*.js'",
    "format:graphql": "prettier --write '**/*.{gql|graphql}'",
    "format:css": "prettier --write '**/*.css'",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "test:coverage": "jest --config jest.test.config.js --coverage --collectCoverageFrom=src/components/**/*.js --collectCoverageFrom=!'src/**/(index|messages).js'",
    "deploy:firebase": "yarn compile-html:firebase && firebase deploy"
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@apollo/react-components": "3.1.5",
    "@apollo/react-hooks": "4.0.0",
    "@babel/helper-regex": "^7.10.5",
    "@commercetools-frontend/actions-global": "20.4.0",
    "@commercetools-frontend/application-components": "20.4.0",
    "@commercetools-frontend/application-shell": "20.4.0",
    "@commercetools-frontend/application-shell-connectors": "20.4.0",
    "@commercetools-frontend/assets": "18.5.4",
    "@commercetools-frontend/constants": "20.3.0",
